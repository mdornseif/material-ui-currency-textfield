{
  "name": "@mdornseif/mui-currency-textfield",
  "version": "1.0.0",
  "description": "Currency input textfield for react with Material-ui style",
  "keywords": [
    "material design",
    "material ui",
    "mui",
    "currency",
    "currency input",
    "currency textfield",
    "react"
  ],
  "homepage": "https://github.com/mdornseif/material-ui-currency-textfield",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdornseif/material-ui-currency-textfield.git"
  },
  "license": "MIT",
  "author": "UNICEF",
  "main": "dist/index.js",
  "module": "dist/mui-currency-textfield.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "deploy": "gh-pages -d styleguide",
    "doc:build": "styleguidist build",
    "doc:publish": "npx gh-pages -m \"[ci skip] Updates\" -d docs",
    "lint": "dts lint",
    "prepare": "husky install",
    "prepublishOnly": "npm run transpile",
    "publish-demo": "npm run build && npm run deploy",
    "release": "npx standard-version",
    "size": "size-limit",
    "start": "dts watch",
    "styleguide": "styleguidist server",
    "test": "dts test --passWithNoTests",
    "unimported": "npx findead src ; npx unimported"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "lint-staged": {
    "*{css,scss,md,markdown,json,yaml,yml,graphql,html}": "prettier -w",
    "*{js,jsx,ts,tsx}": [
      "prettier -w",
      "yarn lint --fix"
    ],
    "package.json": "sort-package-json"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "dependencies": {
    "autonumeric": "^4.5.4",
    "webpack": "^5.85.1"
  },
  "devDependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.2.7",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@tsconfig/create-react-app": "^1.0.3",
    "@tsconfig/recommended": "^1.0.2",
    "@types/node": "^12.6.8",
    "@types/react": "^18.2.4",
    "@types/react-dom": "^18.2.4",
    "core-util-is": "^1.0.2",
    "dts-cli": "^2.0.3",
    "file-loader": "^4.1.0",
    "gh-pages": "^2.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "prop-types": "^15.7.2",
    "react": "^18.2.4",
    "react-dom": "^18.2.4",
    "react-router-dom": "^5.0.1",
    "react-select": "^3.0.4",
    "react-styleguidist": "^10.4.2",
    "size-limit": "^8.2.4",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "@mui/material": "^5.2.7",
    "prop-types": "^15.7.2",
    "react": "^17.0.2 || ^18.1.0",
    "react-dom": "^17.0.2 || ^18.1.0"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "limit": "10 KB",
      "path": "dist/mui-currency-textfield.cjs.production.min.js"
    },
    {
      "limit": "10 KB",
      "path": "dist/mui-currency-textfield.esm.js"
    }
  ]
}
